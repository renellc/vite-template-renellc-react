{
  "$schema": "https://biomejs.dev/schemas/1.8.1/schema.json",
  "vcs": {
    "enabled": true,
    "clientKind": "git",
    "defaultBranch": "develop",
    "useIgnoreFile": true
  },
  "organizeImports": {
    "enabled": true,
    "ignore": ["./src/routeTree.gen.ts"]
  },
  "formatter": {
    "enabled": true,
    "indentStyle": "space",
    "formatWithErrors": true,
    "ignore": ["./src/routeTree.gen.ts"]
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true
    },
    "ignore": ["./src/routeTree.gen.ts"]
  }
}
